{
  "name": "vt-icon",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-tools/vt-icon"
  },
  "main": "dist/index.vue",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vue-tools/vt-icon/issues"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-vue-jsx",
      "transform-runtime"
    ]
  },
  "homepage": "https://github.com/vue-tools/vt-icon#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.3.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.4",
    "file-loader": "^0.10.1",
    "gh-pages": "^1.0.0",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.9",
    "vue": "^2.1.10",
    "vue-docs": "^1.5.3",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^11.1.3",
    "vue-style-loader": "^2.0.1",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^2.4.1"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "copy": "rm -fr dist && cp -rf src dist",
    "docs": "./node_modules/.bin/vue-docs start",
    "docs:build": "rm -fr docs && ./node_modules/.bin/vue-docs build && node gh-pages.js",
    "compile": "npm run copy && node_modules/.bin/babel dist -d dist"
  }
}
